{
    "version": 2,
    "builds": [
      { "src": "/server/**", "use": "@vercel/node" },
      { "src": "/(.*)", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }